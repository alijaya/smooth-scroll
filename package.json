{
  "name": "vue-fluid-scroll",
  "version": "1.0.0",
  "description": "Vue Component for creating fluid scroll. Sometimes you need to scroll with the scrollbar, but the scrolling not fluid enough (because there's a 'step' when you scroll. You want it smoothly transitioned. This is what you need!",
  "keywords": [
    "vue",
    "component",
    "scroll",
    "scrolling",
    "scrollbar",
    "smooth",
    "fluid"
  ],
  "license": "MIT",
  "homepage": "https://alijaya.github.io/vue-fluid-scroll/",
  "bugs": "https://github.com/alijaya/vue-fluid-scroll/issues",
  "repository": "github:alijaya/vue-fluid-scroll",
  "main": "./src/components/VueFluidScroll.vue",
  "author": "Ali Jaya Meilio Lie <alijayameilio@gmail.com>",
  "files": [
    "dist/*",
    "src/components/*"
  ],
  "scripts": {
    "build-bundle": "vue-cli-service build --dest dist --target lib --name vue-fluid-scroll ./src/components/VueFluidScroll.vue",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-fake-scroll": "^1.0.2",
    "vue-resize": "^0.4.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.5",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.5",
    "@vue/cli-service": "^3.0.0-rc.5",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
